.box{height:100vh;background-color:#f6f6f6;display:flex;flex-direction:column}.box .tabs{display:flex;background-color:white;padding:12px 0;font-size:11px}.box .tabs .tab-item{flex:1;display:flex;justify-content:center;align-items:center;color:#999999}.box .tabs .active{color:#32A1EE}.box .list{flex:1;overflow:auto}.box .list .item{margin-bottom:10px}.box .list .item .good-desc{color:#999999}.box .list .item .good-card{background-color:#ffffff;padding:16px;margin:10px 10px 0px}.box .list .item .btn-height{height:100%}.box .f-b{position:fixed;bottom:0;right:0;left:0}.box .empty{flex:1;display:flex;justify-content:center;align-items:center;flex-direction:column}.box .empty .icon{margin-bottom:30px}.box .empty .icon .img{width:200px;height:200px}
