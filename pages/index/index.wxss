page{background-color:#f6f6f6}.banner .banner-item{background-color:#ddd;display:block;height:400px;width:100%}.add-panel{padding:10px 10px;display:flex;align-items:center;background-color:#ffffff;margin-bottom:20px}.add-panel .local-box{flex:1;display:flex;align-items:center}.add-panel .local-box .local-icon{line-height:1;padding:0 5px;color:#777;margin-right:5px}.add-panel .local-box .local-info .local-name{font-size:15px;color:#333}.add-panel .local-box .local-info .local-dis{font-size:12px;color:#777}.add-panel .out-mod{background-color:#32A1EE;border-radius:25px;display:block;padding:1px;display:flex;align-items:center;line-height:1;position:relative}.add-panel .out-mod .out-mod-item{color:#fff;font-size:14px;padding:10px}.add-panel .out-mod .out-mod-item.active{color:#32A1EE;background-color:#ffffff;border-radius:25px}.logo-list{display:flex;align-items:center;max-width:100%;overflow:hidden}.logo-list .item{flex:1;max-width:150px;margin:0 auto;display:block}.logo-list .item:not(:first-child){margin-left:-100px}.logo-list .item .logo{width:150px;display:block}.tep{display:flex;align-items:center;justify-content:center;position:fixed;bottom:20px;left:50%;transform:translateX(-50%);background-color:rgba(0,0,0,0.5);border-radius:25px}.tep .item{line-height:1;font-size:20px;width:30px;height:30px;display:flex;justify-content:center;align-items:center;color:#fff;border-radius:100%;text-align:center}.tep .value{font-size:30px;color:#fff;padding:0 20px}
