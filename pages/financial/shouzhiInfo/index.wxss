page{background-color:#f6f6f6}.box{background-color:white}.box .top{display:flex;flex-direction:column;justify-content:center;align-items:center;padding:26px 0;margin:0 15px;border-bottom:1px solid #dddddd}.box .top .title{font-size:11px;color:#333333}.box .top .info .fuhao{font-size:14px;color:#323233}.box .top .info .money{font-size:30px;color:#333333}.box .bottom .cell{display:flex;flex-direction:row;padding-top:12px;margin:0 15px;font-size:14px}.box .bottom .cell .left{flex:1;color:#666666}.box .bottom .cell .right{color:#333333;text-align:right}.box .bottom .cell:nth-child(4){padding-bottom:10px}
