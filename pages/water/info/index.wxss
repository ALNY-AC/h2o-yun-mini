page{background-color:#f6f6f6;padding-bottom:100px}.box{padding:15px}.box .data-panel{background:linear-gradient(180deg, #61cff8 0%, #32a1ee 100%);color:white;margin-bottom:15px;border-radius:5px}.box .data-panel .value-cell-group{display:flex;flex-wrap:nowrap;justify-content:center;align-items:center}.box .data-panel .value-cell-group:nth-child(1){border-bottom:1px solid rgba(255,255,255,0.5)}.box .data-panel .value-cell-group .value-cell{text-align:center;flex:1;padding:15px}.box .data-panel .value-cell-group .value-cell:nth-child(2){border-left:1px solid rgba(255,255,255,0.5)}.box .data-panel .value-cell-group .value-cell .title{font-size:14px;margin-bottom:10px}.box .data-panel .value-cell-group .value-cell .value{font-size:20px}.goods-list{display:flex;align-items:center;overflow:auto;padding:20px}.goods-list .goods-item{margin-right:20px;width:100px}.goods-list .goods-item .goods-title{overflow:hidden;text-overflow:ellipsis;white-space:nowrap;font-size:11px;color:#333;max-width:100%}.user-card{display:flex;padding:10px 20px;align-items:center;border-bottom:1px solid #f6f6f6}.user-card:last-child{border-bottom:none}.user-card .info-box{flex:1;margin-left:10px}.user-card .info-box .name{font-size:14px;color:#333}.user-card .info-box .time{font-size:11px;color:#666}.user-card .data-box .price{font-size:14px;color:#f00}.user-card .data-box .count{font-size:11px;color:#666}.f-b{position:fixed;bottom:0;left:0;right:0}
